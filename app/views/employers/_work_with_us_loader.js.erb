
(function(){
	var ds=document.createElement('script'); ds.type='text/javascript'; ds.async=true; 
	ds.src='<%=work_with_us_tab_employer_url(@employer.reference_num)%>';
	ds.src+='?host=' + encodeURIComponent(location.hostname);
	var es=document.getElementsByTagName('script')[0];
	ds.onload = function(){
		var fyi = FYI_workWithUs(); 
		if (fyi.positionsCount() > 0) { fyi.setLauncher(fyi.createTab("custom-tab-class")); }
	};
	es.parentNode.insertBefore(ds,es);
})();
